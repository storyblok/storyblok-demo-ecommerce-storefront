    {
      "id": 1809953,
      "name": "What's in space now?",
      "preset": {
        "_uid": "7880ff31-6ca4-4e29-b11c-0e60a768f4df",
        "text": {
          "type": "doc",
          "content": [
            {
              "type": "bullet_list",
              "content": [
                {
                  "type": "list_item",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "text": "There are currently two Space Stations orbiting the Earth, the International Space Station (ISS) and China's Tiangong Space Station (TSS). ",
                          "type": "text"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "list_item",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "text": "There are three rovers on Mars including the Curiosity, Perseverance, and Zhurong. ",
                          "type": "text"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "list_item",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "text": "Nearly 5,000 active artificial satellites are orbiting the Earth.",
                          "type": "text"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "list_item",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "text": "Over 40 space telescopes of all different types are orbiting the earth.",
                          "type": "text"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "list_item",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "text": "Many other unmanned space missions are actively occurring. ",
                          "type": "text"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
